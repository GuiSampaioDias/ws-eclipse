import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		Locale.setDefault(Locale.US);
		double A, B, MEDIA;
		
		A = sc.nextDouble();
		B = sc.nextDouble();
		MEDIA = (A + B)/2;
		
		System.out.printf("MEDIA = %.5f%n", MEDIA);
		
		
		sc.close();
	}

}
